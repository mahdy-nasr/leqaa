<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Users" table="users">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="Name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="age" type="integer" column="age" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mobile" type="string" column="mobile" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passportNumber" type="string" column="passport_number" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="qrCode" type="integer" column="qr_code" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="admin" type="boolean" column="admin" nullable="false"/>
    <field name="language" type="string" column="language" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
